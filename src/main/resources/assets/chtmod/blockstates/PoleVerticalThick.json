{
    "multipart": [{
            "apply": {
                "model": "chtmod:pole/VerticalThick_post"
            }
        }, {
            "when": {
                "north": "true"
            },
            "apply": {
                "model": "chtmod:pole/VerticalThick_side",
                "uvlock": true
            }
        }, {
            "when": {
                "east": "true"
            },
            "apply": {
                "model": "chtmod:pole/VerticalThick_side",
                "y": 90,
                "uvlock": true
            }
        }, {
            "when": {
                "south": "true"
            },
            "apply": {
                "model": "chtmod:pole/VerticalThick_side",
                "y": 180,
                "uvlock": true
            }
        }, {
            "when": {
                "west": "true"
            },
            "apply": {
                "model": "chtmod:pole/VerticalThick_side",
                "y": 270,
                "uvlock": true
            }
        }
    ]
}
