{
    "multipart": [{
            "when": {
                "facing": "north",
                "open": "false"
            },
            "apply": {
                "model": "chtmod:TicketChecker"
            }
        }, {
            "when": {
                "facing": "east",
                "open": "false"
            },
            "apply": {
                "model": "chtmod:TicketChecker",
                "y": 90
            }
        }, {
            "when": {
                "facing": "south",
                "open": "false"
            },
            "apply": {
                "model": "chtmod:TicketChecker",
                "y": 180
            }
        }, {
            "when": {
                "facing": "west",
                "open": "false"
            },
            "apply": {
                "model": "chtmod:TicketChecker",
                "y": 270
            }
        }, {
            "when": {
                "facing": "north",
                "open": "true"
            },
            "apply": {
                "model": "chtmod:TicketChecker_open"
            }
        }, {
            "when": {
                "facing": "east",
                "open": "true"
            },
            "apply": {
                "model": "chtmod:TicketChecker_open",
                "y": 90
            }
        }, {
            "when": {
                "facing": "south",
                "open": "true"
            },
            "apply": {
                "model": "chtmod:TicketChecker_open",
                "y": 180
            }
        }, {
            "when": {
                "facing": "west",
                "open": "true"
            },
            "apply": {
                "model": "chtmod:TicketChecker_open",
                "y": 270
            }
        }
    ]
}
